  "author": "@vzhufk @stepanvanzuriak",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "11"
  },
  "scripts": {
    "start": "node --experimental-modules index.mjs",
    "start:dev": "nodemon --inspect --experimental-modules index.mjs",
    "postinstall": "link-module-alias",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true"
  },
  "dependencies": {
    "bookshelf": "^0.14.2",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.2.1",
    "knex": "^0.16.3",
    "pg": "^7.9.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "link-module-alias": "^1.1.6",
    "nodemon": "^1.18.10"
  },
  "_moduleAliases": {
    "@src": "src",
    "@config": "config/index.mjs",
    "@helpers": "src/helpers",
    "@log": "log.mjs"
  }
}
